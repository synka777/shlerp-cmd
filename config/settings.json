{
    "rel_logs_path": ".local/logs",
    "verbose": false,
    "logging": {
        "prune": {
            "enabled": true,
            "max_days": 30
        },
        "no_prune": {
            "max_log_size": 50000
        }
    },
    "rules": {
        "history_limit": {
            "frameworks": 2,
            "vanilla": 1
        }
    },
    "upload_default": {
        "expiration": "1Q"
    }
}