{
    "frameworks": [
        {
            "name":"Ionic_cordova",
            "detect":{
                "files":[
                    {
                        "names":["ionic.config.json"],
                        "pattern":null
                    }
                ],
                "folders":[
                    {
                        "name":"node_modules",
                        "files":[]
                    },
                    {
                        "name":"platforms",
                        "files":[]
                    }
                ]
            },
            "actions":{
                "exclude":{
                    "files":[],
                    "folders":[],
                    "dep_folders": ["node_modules"]
                }
            }
        },
        {
            "name":"Ionic_capacitor",
            "detect":{
                "files":[
                    {
                        "names":["ionic.config.json"],
                        "pattern":null
                    },
                    {
                        "names":["capacitor.config.ts"],
                        "pattern":null
                    }
                ],
                "folders":[
                    {
                        "name":"node_modules",
                        "files":[]
                    }
                ]
            },
            "actions":{
                "exclude":{
                    "files":[],
                    "folders":[],
                    "dep_folders": ["node_modules"]
                }
            }
        },
        {
            "name":"React_native",
            "detect":{
                "files":[
                    {
                        "names":["package.json", "package-lock.json"],
                        "pattern":"react-native"
                    }
                ],
                "folders":[
                    {
                        "name":".expo-shared",
                        "files":["assets.json"]
                    },
                    {
                        "name":"node_modules",
                        "files":[]
                    }
                ]
            },
            "actions":{
                "exclude":{
                    "files":[],
                    "folders":[],
                    "dep_folders": ["node_modules"]
                }
            }
        },
        {
            "name":"Flask",
            "detect": {
                "files": [
                    {
                        "names": ["app.py", "main.py", "wsgi.py"],
                        "pattern": null
                    },
                    {
                        "names": ["requirements.txt", "Pipfile"],
                        "pattern": "\\bflask\\b"
                    }
                ],
                "folders": [
                    {
                        "name": "instance",
                        "files": []
                    },
                    {
                        "name": "migrations",
                        "files": []
                    }
                ]
            },
            "actions":{
                "exclude":{
                    "files":[],
                    "folders":["__pycache__", "instance", "migrations"],
                    "dep_folders": ["venv", "env", ".venv"]
                }
            }
        },
        {
            "name":"Spring_Boot",
            "detect":{
                "files":[
                    {
                        "names":["pom.xml", "build.gradle"],
                        "pattern":null
                    }
                ],
                "folders":[
                    {
                        "name":"target",
                        "files":[ ]
                    },
                    {
                        "name":"build",
                        "files":[ ]
                    }
                ]
            },
            "actions":{
                "exclude":{
                    "files":[],
                    "folders":["target", "build"],
                    "dep_folders": []
                }
            }
        },
        {
            "name":"Laravel",
            "detect":{
                "files":[
                    {
                        "names":["artisan", "composer.json"],
                        "pattern":null
                    }
                ],
                "folders":[
                    {
                        "name":"vendor",
                        "files":[ ]
                    },
                    {
                        "name":"storage",
                        "files":[ ]
                    }
                ]
            },
            "actions":{
                "exclude":{
                    "files":[".env"],
                    "folders":["node_modules", "vendor", "storage/logs"],
                    "dep_folders": ["vendor"]
                }
            }
        }
    ],
    "vanilla": [
        {
            "name":"Python",
            "detect":{
                "extensions":[
                    {
                        "names":["*.py"],
                        "weight":0.5
                    }
                ]
            },
            "actions":{
                "exclude":{
                    "files":[],
                    "folders":["__pycache__", "site_packages", "dist", "build"],
                    "dep_folders": [
                        "venv",
                        "env",
                        ".venv",
                        "virtualenv",
                        ".virtualenv", 
                        "pipenv",
                        "poetry",
                        "conda_env"
                    ]
                }
            }
        },
        {
            "name":"Javascript",
            "detect":{
                "extensions":[
                    {
                        "names":["*.js", "*.ts", "*.jsx"],
                        "weight":0.5
                    }
                ]
            },
            "actions":{
                "exclude":{
                    "files":["out.log", "karma.conf.js"],
                    "folders":["buildprep"],
                    "dep_folders": ["node_modules"]
                }
            }
        },
        {
            "name":"Java",
            "detect":{
                "extensions":[
                    {
                        "names":["*.class", "*.java", "*.jar"],
                        "weight":0.5
                    }
                ]
            },
            "actions":{
                "exclude":{
                    "files":[],
                    "folders":[],
                    "dep_folders": null
                }
            }
        },
        {
            "name":"Rust",
            "detect":{
                "extensions":[
                    {
                        "names":["*.rs"],
                        "weight":0.5
                    }
                ],
                "files":[
                    {
                        "names":["Cargo.toml", "Cargo.lock"],
                        "pattern":null
                    }
                ]
            },
            "actions":{
                "exclude":{
                    "files":[],
                    "folders":["target"],
                    "dep_folders": []
                }
            }
        },
        {
            "name":"PHP",
            "detect":{
                "extensions":[
                    {
                        "names":["*.php"],
                        "weight":0.5
                    }
                ],
                "files":[
                    {
                        "names":["composer.json", "composer.lock"],
                        "pattern":null
                    }
                ]
            },
            "actions":{
                "exclude":{
                    "files":[],
                    "folders":["vendor"],
                    "dep_folders": ["vendor"]
                }
            }
        }
    ]
}